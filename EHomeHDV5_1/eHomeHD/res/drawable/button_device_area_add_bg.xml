<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
            android:bottom="1dp"
            android:left="1dp"
            android:right="1dp"
            android:top="1dp">
        <shape>
            <stroke
                    android:dashGap="5dp"
                    android:dashWidth="5dp"
                    android:width="1dp"
                    android:color="@color/v5_green_light" />

            <solid android:color="#FFFFFF" />

            <corners android:radius="5dp" />
        </shape>
    </item>

</layer-list>